{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Flutter",
        "type": "dart",
        "request": "launch",
        "program": "lib/main.dart"
      },
      {
        "name": "Flutter Web (chrome)",
        "type": "dart",
        "request": "launch",
        "program": "lib/main.dart",
        "args": [
          "-d",
          "chrome"
        ],
      },
      {
        "name": "Flutter Web (WebServer)",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "flutterMode": "debug",
        "args": [
          "-d",
          "web-server",
          "--web-port=8000",
          "--web-hostname=localhost"
        ],
      },
      {
        "name": "Flutter Web (Mobile)",
        "request": "launch",
        "type": "dart",
        "args": [
          "-d",
          "web-server",
          "--web-hostname",
          "0.0.0.0",
          "--web-port",
          "8000"
        ],
      },
      {
        "name": "Flutter Web (Profile)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile",
        "args": [
          "-d",
          "chrome",
          "--profile"
        ],
      },
      {
        "name": "Flutter Web Attach",
        "request": "attach",
        "type": "dart"
      }
    ]
  }
  